{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["google", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "require-jsdoc": "off",
    "no-unused-vars": "warn"
  }
}
